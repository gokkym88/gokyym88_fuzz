#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rclpy-create-publisher','console_scripts','rclpy_create_publisher'
__requires__ = 'rclpy-create-publisher'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rclpy-create-publisher', 'console_scripts', 'rclpy_create_publisher')()
    )
