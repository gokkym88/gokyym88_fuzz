#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rclpy-subscriber','console_scripts','rclpy_subscriber'
__requires__ = 'rclpy-subscriber'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rclpy-subscriber', 'console_scripts', 'rclpy_subscriber')()
    )
