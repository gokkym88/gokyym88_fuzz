#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rclpy-create-node','console_scripts','rclpy_create_node'
__requires__ = 'rclpy-create-node'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rclpy-create-node', 'console_scripts', 'rclpy_create_node')()
    )
