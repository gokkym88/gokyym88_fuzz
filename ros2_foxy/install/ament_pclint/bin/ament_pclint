#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ament-pclint','console_scripts','ament_pclint'
__requires__ = 'ament-pclint'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ament-pclint', 'console_scripts', 'ament_pclint')()
    )
